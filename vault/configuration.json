{
  "disable_mlock": true,
  "default_lease_ttl": "24h",
  "max_lease_ttl": "24h",
  "backend": {
    "consul": {
      "path": "vault/",
      "address": "127.0.0.1:8500",
      "scheme": "http",
      "datacenter": "dc1"
    }
  },
  "listener": {
    "tcp": {
      "address": "127.0.0.1:8200",
      "tls_cert_file": "/var/lib/vault/ssl/vault.crt",
      "tls_key_file": "/var/lib/vault/ssl/vault.key"
    }
  }
}